{
    "name": "ryan/linekit",
    "type": "library",
    "description": "指令搭建工具",
    "authors": [
        {
            "name": "Ryan",
            "email": "apple040285.1@gmail.com"
        }
    ],
    "repositories": {
        "main": {
            "type": "git",
            "url": "git@gitlab.com:ici2/full-linebot.git"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "6.0",
        "laravel/framework": "^8.12",
        "laravel/chillbot": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Ryan\\LineKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryan\\LineKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ryan\\LineKit\\KitServiceProvider"
            ]
        }
    }
}
